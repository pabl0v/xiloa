	<queryString language="SQL">
		<![CDATA[select c.certificacion_nombre as curso_nombre,
       uc.nombre_completo as candidato_nombre,
       un.descripcion as unidad_nombre,
       te.centroid,
       td.id_matricula,
       ld.nombres,
       ld.apellidos,
       ld.cedula,
       ld.id_estudiante,
       ld.id_libro,
       ld.tipo_libro,
       ld.tomo,
       ld.registro,
       ld.folio
from sccl.solicitudes s, sccl.certificaciones c, sccl.contactos uc, sccl.certificacion_unidades u, sccl.evaluaciones_unidad eu,
     registro_cobranza.cu_cat_uc un, scert.tramites_d td, scert.tramites_e te, scert.libro_detalle ld
where s.solicitud_id = $P{idSolicitud}
 and uc.contacto_id = s.contacto_id
 and c.certificacion_id = s.certificacion_id
 and u.certificacion_id = c.certificacion_id
 and eu.evaluacion_unidad_solicitud_id = s.solicitud_id
 and eu.evaluacion_ucl_id = u.unidad_id
 and eu.evaluacion_unidad_aprobado = true
 and un.id = eu.evaluacion_ucl_id
 and td.id_matricula = s.id_matricula
 and te.centroid = $P{idCentro}
 and td.no_tramite = te.no_tramite
 and td.no_tramite = ld.no_tramite
 and td.id = ld.id_estudiante]]>
