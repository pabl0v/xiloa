	<queryString language="SQL">
		<![CDATA[select s.fecha_registro as solicitud_fecha,
       s.fecha_matricula as solicitud_fmatricula,
       s.experiencia as solicitud_experiencia,
       s.ocupacion as solicitud_oficio,
       s.empresa as solicitud_empresa,
      ( select extract(year from s.fecha_registro)) as solicitud_anio,
      (select extract(month from s.fecha_registro)) as solicitud_mes,
      (select extract(days from s.fecha_registro)) as solicitud_dia,
       c.certificacion_nombre as certificacion_nombre,
       c.certificacion_ifp_nombre as certificacion_ifpnombre,
       cu.primer_nombre as contacto_nombre1,
       cu.segundo_nombre as contacto_nombre2,
       cu.primer_apellido as contacto_apellido1,
       cu.segundo_apellido as contacto_apellido2,
       cu.numero_identificacion as contacto_nui,
       cu.telefono1 as telefono,
       cu.telefono2 as celular,
       cu.direccion_actual as dir_domicilio,
       m.nombre as contacto_municipio,
       g.mantenedor_valor
from sccl.solicitudes s, sccl.certificaciones c, sccl.contactos cu, public.municipio m,
     (select mant.* from sccl.mantenedores mant where mant.mantenedor_tipo = '10') g
where solicitud_id = 1
 and c.certificacion_id = s.certificacion_id
 and cu.contacto_id = s.contacto_id
 and m.departamentoid = cu.departamento_id
 and m.municipioid = cu.municipio_id
 and g.mantenedor_id = cu.sexo_id]]>
	</queryString>
