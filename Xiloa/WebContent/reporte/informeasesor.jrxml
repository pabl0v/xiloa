	<queryString language="SQL">
		<![CDATA[select c.certificacion_ifp_nombre as centro_nombre,
       c.certificacion_nombre as curso_nombre,
       cc.nombre_completo as candidato_nombre,
       cc.numero_identificacion as candidato_identificacion,
       cc.direccion_actual as candidato_direccion,
       cc.telefono1 as candidato_telefono,
       cc.telefono2 as candidato_celular,
       cc.correo1 as candidato_email,
       evaluador.nombre_completo as evaluador_nombre,
       evaluador.numero_identificacion as evaluador_identificacion,
       evaluador.direccion_actual as evaluador_direccion,
       evaluador.telefono1 as evaluador_telefono,
       evaluador.telefono2 as evaluador_celular,
       evaluador.correo1 as evaluador_email,
       un.id as unidad_identificador,
       un.descripcion as unidad_descripcion
from sccl.solicitudes s, sccl.certificaciones c,
     (select ce.* from sccl.contactos ce where ce.contacto_id = $P{idEvaluador} ) evaluador,
     sccl.contactos cc, sccl.certificacion_unidades u, registro_cobranza.cu_cat_uc un
where s.solicitud_id = $P{idSolicitud}
 and c.certificacion_id = s.certificacion_id
 and cc.contacto_id = s.contacto_id
 and u.certificacion_id = c.certificacion_id
 and un.id = u.unidad_id]]>
	</queryString>
