	<queryString><![CDATA[select	distinct c.contacto_id,p.certificacion_ifp_nombre as nombre_centro,
	cv.fecha_programacion as fecha_convocatoria,
	c.primer_apellido,
	c.segundo_apellido,
	c.primer_nombre,
	c.segundo_nombre,
	c.municipio_id as lugar,
	to_char(age (current_date, c.fecha_nacimiento),'YY') as edad,
	case when c.sexo_id=1 then 'X' else null end as masculino,
	case when c.sexo_id=0 then 'X' else null end as femenino,
	case when s.situacion_laboral=true then 'Labora' else 'No labora' end as situacion_laboral,
	r.descripcion_nivel as nivel_academino,
	(array_to_string(array(select uc.cod_cualificacion from registro_cobranza.cu_cat_uc uc where uc.id in (
	select unidad_id from sccl.unidades u where u.certificacion_id=p.certificacion_id)), ', ')) as unidad_competencia,
	c.telefono1,c.telefono2,c.direccion_actual,numero_identificacion as cedula,
	case when r.id_nivel_academico=1 then 'X' else '' end as primaria,
	case when (r.id_nivel_academico=2 or r.id_nivel_academico=5) then 'X' else '' end as secundaria,
	case when (r.id_nivel_academico=3 or r.id_nivel_academico=4 or r.id_nivel_academico=7) then 'X' else '' end as universidad,
	case when r.id_nivel_academico=6 then 'X' else '' end as tecnico
from	sccl.certificaciones p
	inner join
	sccl.solicitudes s
	on s.certificacion_id=p.certificacion_id
	and s.solicitud_estatus=39
	and p.certificacion_estatus!=18
	inner join
	sccl.contactos c
	on s.contacto_id=c.contacto_id
	inner join
	registro_cobranza.catalogo_nivel_academico r
	on s.escolaridad_id=id_nivel_academico
	inner join
	sccl.convocatorias cv
	on cv.solicitud_id=s.solicitud_id
	inner join
	sccl.actividades a
	on cv.actividad_id=a.actividad_id
	and a.actividad_tipo_id=6]]></queryString>

